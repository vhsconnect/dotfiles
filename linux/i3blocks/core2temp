#! /bin/bash

MonDir='/sys/class/hwmon/hwmon3'

core1=`< "$MonDir/temp2_input"`
echo " ïƒˆ  "
echo "323"
if [ "$core1" -ge "80000" ]; then
 echo "#EE3E38"
elif [ "$core1" -ge "70000" ]; then
 echo "#F86E51"
elif [ "$core1" -ge "60000" ]; then
 echo "#FBA465"
elif [ "$core1" -ge "500000" ]; then
 echo "#F2C85B"
else echo "#00FA9A"
fi
